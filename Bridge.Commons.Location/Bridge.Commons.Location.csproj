<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageId>Bridge.Commons.Location</PackageId>
        <Title>Bridge.Commons.Location</Title>
        <Version>1.0.0</Version>
        <Authors>Robson Leao</Authors>
        <Company>Bridge Core</Company>
        <Description>
            Location package: Timezone, Location, Globalization, DateTime.
        </Description>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageReleaseNotes>First Version</PackageReleaseNotes>
        <PackageTags>bridge-core commons location date timezone</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="GeoTimeZone" Version="4.1.0"/>
        <PackageReference Include="NodaTime" Version="3.0.9"/>
    </ItemGroup>

</Project>